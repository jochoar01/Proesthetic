CREATE TABLE Clinicas(
  `idClinicas` INT NOT NULL AUTO_INCREMENT,
  `NitClinicas` varchar(15) DEFAULT NULL,
  `NombreClinica` VARCHAR(45) NULL,
  `DireccionClinica` VARCHAR(45) NULL,
  `TelefonoClinica` VARCHAR(45) NULL,
  `Habilitado` TINYINT(1) NULL,
  PRIMARY KEY (`idClinicas`),
  UNIQUE INDEX `idClinicas_UNIQUE` (`idClinicas` ASC));




CREATE TABLE Odontologos (
  `idOdontologos` INT UNSIGNED NOT NULL AUTO_INCREMENT,
  `NombreOdontologo` VARCHAR(45) NULL,
  `TelefonoOdontologo` VARCHAR(45) NULL,
  `EmailOdontologo` varchar(45) DEFAULT NULL,
  `Habilitado` TINYINT(1) NULL,
  PRIMARY KEY (`idOdontologos`),
  UNIQUE INDEX `idOdontologos_UNIQUE` (`idOdontologos` ASC));



CREATE TABLE Usuarios (
  `idUsuarios` INT UNSIGNED NOT NULL AUTO_INCREMENT,
  `NombreUsuario` VARCHAR(45) NULL,
  `Nombre` VARCHAR(45) NULL,
  `Rol` VARCHAR(45) NULL,
  `Password` VARCHAR(45) NULL,
  `Habilitado` TINYINT(1) NULL,
  PRIMARY KEY (`idUsuarios`),
  UNIQUE INDEX `idUsuarios_UNIQUE` (`idUsuarios` ASC));

