CREATE TABLE pedidos (
  `idpedidos` INT NOT NULL AUTO_INCREMENT UNIQUE,
  `caja` VARCHAR(45),
  `clinica` VARCHAR(45),
  `paciente` VARCHAR(45),
  `orden` VARCHAR(45),
  `antagonista` VARCHAR(45),
  `fecha_entrada` VARCHAR(45),
  `odontologo` VARCHAR(45),
  `tipo_trabajo` VARCHAR(45),
  `fecha_entrega` VARCHAR(45),
  `prueba1` VARCHAR(45),
  `prueba2` VARCHAR(45),
  `prueba3` VARCHAR(45),
  `Habilitado` TINYINT(1),
  PRIMARY KEY (`idpedidos`));

